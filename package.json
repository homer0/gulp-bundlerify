{
  "name": "gulp-bundlerify",
  "version": "1.0.0",
  "devDependencies": {
    "gulp-util": "3.0.7",
    "babel-cli": "6.0.2",
    "babel-preset-es2015": "6.0.11",
    "babelify": "7.0.1",
    "browser-sync": "2.9.11",
    "browserify": "12.0.1",
    "rimraf": "2.4.3",
    "watchify": "3.6.0",
    "vinyl-source-stream": "1.1.0",
    "whatwg-fetch": "0.10.0",
    "core-js": "1.2.3",
    "gulp-if": "2.0.0",
    "gulp-uglify": "1.4.2",
    "gulp-streamify": "1.0.2",
    "object-assign-deep": "0.0.4",
    "jest-cli": "0.5.10",
    "babel-jest": "5.3.0",
    "gulp": "3.9.0",
    "babel-eslint": "4.1.1",
    "eslint": "1.7.1",
    "jscs": "2.5.0",
    "esdoc": "0.4.1",
    "esdoc-es7-plugin": "v0.0.2"
  },
  "scripts": {
    "build": "babel --presets es2015 -d dist/ src/",
    "prepublish": "npm run build",
    "test": "jest ./tests/index-test.js",
    "lint": "eslint ./src/ ./tests/; jscs ./src/ ./tests/",
    "docs": "esdoc -c esdoc.json; open docs/index.html"
  },
  "jest": {
    "collectCoverage": true,
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "testDirectoryName": "tests",
    "testFileExtensions": [
      "es6",
      "js",
      "jsx"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx",
      "es6"
    ],
    "globals": {
      "BrowserifyMockPath": "./browserify.js"
    }
  },
  "author": "homer0",
  "license": "MIT",
  "main": "dist/index.js"
}
